{
  "name": "project-creation",
  "version": "1.0.0",
  "description": "Lambda function to automate project creation in infra",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && tsc -b && rm -rf ./artefact && mkdir artefact && (cd build && zip -r ../artefact/build.zip *)",
    "test": "npm run lint && npm run start-containers && sleep 2 && npm run mocha-test && npm run stop-containers",
    "mocha-test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "start-containers": "docker-compose up -d",
    "stop-containers": "docker-compose down",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auroraai-bot-platform/project-creation.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/auroraai-bot-platform/project-creation/issues"
  },
  "homepage": "https://github.com/auroraai-bot-platform/project-creation#readme",
  "dependencies": {
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "mocha": "^9.2.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
